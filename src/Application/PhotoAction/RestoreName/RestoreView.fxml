<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<GridPane fx:controller="Application.PhotoAction.RestoreName.RestoreController"
          xmlns:fx="http://javafx.com/fxml" alignment="top_left" hgap="10" vgap="10">
    <padding>
        <Insets top="25" right="25" bottom="25" left="25"/>
    </padding>

    <Text text="Pick a name:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <ChoiceBox fx:id="namesToChoose" prefWidth="200.0" GridPane.rowIndex="1" GridPane.columnSpan="2"/>
    <Button text="Restore Name" onAction="#handleRestoreName" GridPane.columnIndex="0" GridPane.rowIndex="2"/>

</GridPane>
